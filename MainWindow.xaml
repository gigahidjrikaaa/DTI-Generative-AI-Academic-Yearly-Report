<Window x:Class="DTI_Generative_AI_Academic_Yearly_Report.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DTI_Generative_AI_Academic_Yearly_Report.Helper"
        xmlns:vm="clr-namespace:DTI_Generative_AI_Academic_Yearly_Report.ViewModel"
        mc:Ignorable="d"
        Title="Acad Yearly Report" 
        WindowStyle="None"
        x:Name="AcadYearlyReportWindow"
        Height="800"
        Width="1350"
        MinHeight="400"
        MinWidth="600"
        AllowsTransparency="True"
        ResizeMode="CanResizeWithGrip"
        WindowStartupLocation="CenterScreen"
        Background="Transparent"
        >
    <Grid>
        <Border
            Background="{StaticResource DTI-Blue}"
            BorderBrush="{StaticResource DTI-White}"
            BorderThickness="2"
            CornerRadius="10, 10, 0, 10"/>

    </Grid>
</Window>
